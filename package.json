{
  "name": "normalize",
  "version": "0.0.0",
  "description": "Normalize file for Mol Framework",
  "scripts": {
    "dist": "sass scss/normalize.scss:dist/normalize.min.css --style compressed",
    "docs": "npm run scss | npm run server",
    "server": "browser-sync start --server 'docs' --files 'docs'",
    "scss": "sass --watch scss/normalize.scss:docs/normalize.css --style expanded"
  },
  "keywords": [
    "mol",
    "mol-framework",
    "css",
    "sass",
    "scss",
    "scss-module",
    "normalize"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MolFramework/normalize.git"
  },
  "author": "flkt <flkt.crnpio@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MolFramework/normalize/issues"
  },
  "devDependencies": {
    "browser-sync": "^2.26.3"
  }
}
